            },
            "environment": {
                "RUSTC_WRAPPER": "sccache"
            }
        },
        {
            "name": "qt5",
            "hidden": true,
            "cacheVariables": {
                "USE_QT5": true
            }
        },
        {
            "name": "vcpkg",
            "hidden": true,
