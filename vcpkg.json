{
  "name": "cxx-qt",
  "version": "0.3",
  "description": "Rust bindings for Qt using CXX",
  "homepage": "https://kdab.github.io/cxx-qt/book/",
  "license": "MIT OR Apache-2.0",
  "supports": "!uwp",
  "features": {
    "qt5": {
      "description": "Build with Qt5",
      "dependencies": [
        "qt5-base",
        "qt5-declarative",
        "qt5-quickcontrols2"
      ]
    },
    "qt6": {
      "description": "Build with Qt6",
      "dependencies": [
        "qtbase",
        "qtdeclarative",
        "qtquickcontrols2"
      ]
    }
  }
}
